func uniquePathsWithObstacles(obstacleGrid [][]int) int {
	m := len(obstacleGrid)
	n := len(obstacleGrid[0])
	matrix := make([][]int, m+1)
	for i := 0; i < m+1; i++ {
		matrix[i] = make([]int, n+1)
	}
	matrix[m][n-1] = 1
	for i := m - 1; i >= 0; i-- {
		for j := n - 1; j >= 0; j-- {
			if obstacleGrid[i][j] == 1 {
				continue
			}
			matrix[i][j] = matrix[i+1][j] + matrix[i][j+1]
		}
	}
	return matrix[0][0]
}
